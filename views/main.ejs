<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Main page</title>
  </head>
  <body>
    <%- include('header', {headers}) %>
    <main>
      <% if (locals.sections) { %> <% for(let section of locals.sections) { %>
      <%- include('sections', {items:section.items, title:section.title}) %>
      <%}%> <% } %> <% if (locals.title) { %> <%- include('addNewItem', { title:
      locals.title, columns: locals.columns }) %> <% } %>
    </main>
  </body>
</html>
